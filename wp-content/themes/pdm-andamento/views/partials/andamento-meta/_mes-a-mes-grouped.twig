{% if progresso | length > 0 %}

        <h5>Agrupado</h5>
        <ul class="tabs tab-options inline-list small-2 columns" id="tab-por-ano-na-meta" data-tab>
            <li class="tab-title"><a href="#panel2-1">2013</a></li>
            <li class="tab-title active"><a href="#panel2-2">2014</a></li>
        </ul>
        <div class="tabs-content">
            <div class="content" id="panel2-1">
                <div class="chart-render chart-container chart-container-projeto line-container" id="chart-month-a" data-chart="{&quot;type&quot;:&quot;line&quot;}"></div>
                <script>
                    var data_chart = {
                        "type" : "line",
                        "labels" : [
                                    'Janeiro',
                                    'Fevereiro',
                                    'Março',
                                    'Abril',
                                    'Maio',
                                    'Junho',
                                    'Julho',
                                    'Agosto',
                                    'Setembro',
                                    'Outubro',
                                    'Novembro',
                                    'Dezembro'
                        ],
                        "values" : [
                                    [{{projects.agrupado_total.2013|join(",")}}]
                        ]
                    };
                </script>
            </div>
            <div class="content active" id="panel2-2">
                <div class="chart-render chart-container chart-container-projeto line-container" id="chart-month-b" data-chart="{&quot;type&quot;:&quot;line2&quot;}"></div>

                <script>
                    var data_chart2 = {
                        "type" : "line",
                        "labels" : [
                                    'Janeiro',
                                    'Fevereiro',
                                    'Março',
                                    'Abril',
                                    'Maio',
                                    'Junho',
                                    'Julho',
                                    'Agosto',
                                    'Setembro',
                                    'Outubro',
                                    'Novembro',
                                    'Dezembro'
                        ],
                        "values" : [
                                    [{{projects.agrupado_total.2014|join(",")}}]
                        ]
                    };
                </script>
            </div>
        </div>
{% endif %}

